ction(movie) {\n        const row = '<tr>' +\n            '<td>' + movie.title + '</td>' +\n            '<td>' + movie.year + '</td>' +\n            '<td>' + movie.director + '</td>' +\n            '<td>' + movie.genres + '</td>' +\n            '<td>' + movie.stars + '</td>' +\n            '<td>' + movie.rating + '</td>' +\n            '</tr>';\n        tbody.append(row);\n    });\n    // Show the search results table\n    $('#movie_table').show();\n}\n
